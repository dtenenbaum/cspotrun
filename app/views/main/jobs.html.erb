



My Jobs:<br/>


<table>
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th>Time Started</th>
  </tr>

<% for job in @jobs %>
  <tr>
    <td><%= link_to job.id, :action => "events", :job_id => job.id %></td>
    <td><%= job.name %></td>
    <td><%= job.created_at %></td>
  </tr>
<% end %>
</table>
<br/>
<%= will_paginate @jobs %>


